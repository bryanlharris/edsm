{
  "name": "edsm",
  "email_notifications": {},
  "webhook_notifications": {},
  "timeout_seconds": 0,
  "trigger": {
    "pause_status": "UNPAUSED",
    "file_arrival": {
      "url": "/Volumes/edsm/bronze/landing/",
      "min_time_between_triggers_seconds": 0
    }
  },
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "job_settings",
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "01_job_settings",
        "source": "GIT"
      },
      "timeout_seconds": 0,
      "email_notifications": {}
    },
    {
      "task_key": "bronze_loop",
      "depends_on": [
        {
          "task_key": "job_settings"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "for_each_task": {
        "inputs": "{{tasks.job_settings.values.bronze}}",
        "concurrency": 4,
        "task": {
          "task_key": "bronze",
          "run_if": "ALL_SUCCESS",
          "notebook_task": {
            "notebook_path": "05_ingest",
            "base_parameters": {
              "job_settings": "{{input}}",
              "color": "bronze"
            },
            "source": "GIT"
          },
          "max_retries": 0,
          "min_retry_interval_millis": 900000,
          "retry_on_timeout": false,
          "disable_auto_optimization": true,
          "timeout_seconds": 0,
          "email_notifications": {}
        }
      },
      "timeout_seconds": 0,
      "email_notifications": {}
    },
    {
      "task_key": "silver_loop",
      "depends_on": [
        {
          "task_key": "bronze_loop"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "for_each_task": {
        "inputs": "{{tasks.job_settings.values.silver}}",
        "concurrency": 4,
        "task": {
          "task_key": "silver",
          "run_if": "ALL_SUCCESS",
          "notebook_task": {
            "notebook_path": "05_ingest",
            "base_parameters": {
              "job_settings": "{{input}}",
              "color": "silver"
            },
            "source": "GIT"
          },
          "max_retries": 0,
          "min_retry_interval_millis": 900000,
          "retry_on_timeout": false,
          "disable_auto_optimization": true,
          "timeout_seconds": 0,
          "email_notifications": {}
        }
      },
      "timeout_seconds": 0,
      "email_notifications": {}
    },
    {
      "task_key": "gold_loop",
      "depends_on": [
        {
          "task_key": "silver_loop"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "for_each_task": {
        "inputs": "{{tasks.job_settings.values.gold}}",
        "concurrency": 4,
        "task": {
          "task_key": "gold",
          "run_if": "ALL_SUCCESS",
          "notebook_task": {
            "notebook_path": "05_ingest",
            "base_parameters": {
              "job_settings": "{{input}}",
              "color": "gold"
            },
            "source": "GIT"
          },
          "max_retries": 0,
          "min_retry_interval_millis": 900000,
          "retry_on_timeout": false,
          "disable_auto_optimization": true,
          "timeout_seconds": 0,
          "email_notifications": {}
        }
      },
      "timeout_seconds": 0,
      "email_notifications": {}
    },
    {
      "task_key": "history_loop",
      "depends_on": [
        {
          "task_key": "gold_loop"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "for_each_task": {
        "inputs": "{{tasks.job_settings.values.history_settings}}",
        "concurrency": 11,
        "task": {
          "task_key": "history",
          "run_if": "ALL_SUCCESS",
          "notebook_task": {
            "notebook_path": "06_history",
            "base_parameters": {
              "job_settings": "{{input}}"
            },
            "source": "GIT"
          },
          "max_retries": 0,
          "min_retry_interval_millis": 900000,
          "retry_on_timeout": false,
          "disable_auto_optimization": true,
          "timeout_seconds": 0,
          "email_notifications": {}
        }
      },
      "timeout_seconds": 0,
      "email_notifications": {}
    },
    {
      "task_key": "optimize_and_vacuum",
      "depends_on": [
        {
          "task_key": "history_loop"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "08_optimize_and_vacuum",
        "source": "GIT"
      },
      "max_retries": 0,
      "min_retry_interval_millis": 900000,
      "retry_on_timeout": false,
      "disable_auto_optimization": true,
      "timeout_seconds": 0,
      "email_notifications": {}
    }
  ],
  "git_source": {
    "git_url": "https://github.com/bryanlharris/edsm.git",
    "git_provider": "gitHub",
    "git_branch": "main"
  },
  "queue": {
    "enabled": true
  },
  "performance_target": "PERFORMANCE_OPTIMIZED",
  "run_as": {
    "user_name": "bryanlharris@me.com"
  }
}
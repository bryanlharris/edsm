{
  "name": "edsm-workspace",
  "email_notifications": {
    "no_alert_for_skipped_runs": false
  },
  "webhook_notifications": {},
  "timeout_seconds": 0,
  "trigger": {
    "pause_status": "PAUSED",
    "file_arrival": {
      "url": "/Volumes/edsm/bronze/landing/",
      "min_time_between_triggers_seconds": 0
    }
  },
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "job_settings",
